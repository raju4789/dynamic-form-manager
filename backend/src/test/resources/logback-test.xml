<configuration>
    <!-- Suppress logs for the service package -->
    <logger name="com.finclutech.dynamic_form_manager.services" level="OFF" />

    <!-- Console appender -->
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
</configuration>